<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
     <servlet-name>add</servlet-name>
     <servlet-class>com.mobile.AddMobile</servlet-class>
  </servlet>
  <servlet>
     <servlet-name>updated</servlet-name>
     <servlet-class>com.mobile.UpdatedMob</servlet-class>
  </servlet>
    <servlet>
     <servlet-name>deleted</servlet-name>
     <servlet-class>com.mobile.DeleteMob</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
     <servlet-name>add</servlet-name>
     <url-pattern>/addMobile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
     <servlet-name>updated</servlet-name>
     <url-pattern>/UpdatedMobileData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
     <servlet-name>deleted</servlet-name>
     <url-pattern>/DeleteMobile</url-pattern>
  </servlet-mapping>
</web-app>
